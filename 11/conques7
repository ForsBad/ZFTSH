var 
    a,b,x,xnew:integer ;
begin 
    xnew := 0;
    a := 0;
    b := 0;
    while (a<>5) or (b <> 9) do 
        begin
            a := 0;
            b := 0;
            x := xnew;
            xnew := xnew +1;
            writeln(x);

        while x > 0 do begin
            if x mod 2 = 0 then
                a:= a + x mod 3
            else
                b:= b + x mod 5;
                x:= x div 3;

        end;
    end;
end.
